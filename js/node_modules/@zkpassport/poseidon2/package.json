{
  "name": "@zkpassport/poseidon2",
  "version": "0.6.2",
  "description": "A blazing fast Poseidon2 implementation in TypeScript for the BN254 curve",
  "license": "MIT",
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "test": "bun test tests/*.test.ts",
    "coverage": "bun test tests/*.test.ts --coverage",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier . --write",
    "build": "bun run build:cjs && bun run build:esm",
    "build:cjs": "tsup",
    "build:esm": "rimraf ./dist/esm && tsc && bun run scripts/fix-aliases.mjs",
    "prepublishOnly": "bun run lint && bun run test && bun run build"
  },
  "devDependencies": {
    "@types/bun": "^1.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3"
  }
}
